//=====================================================================================================
function lista_resp(codprof) {
    $.post('/get_respempresas', {elprof:codprof})
      .done(function(data) {
         alert(data)
         for (let i=0; i < data.length; i++) {
               img=escoger_boton(data[i][5])
               $('#tab_respuestas').append('<tbody><tr><td style="width:26%">' + data[i][6] +'</td><td style="width:25%">' + data[i][7] + '</td><td style="width:43%">' + data[i][8]
               +'</td><td style="width:8%"><input type="button" class="smallredondo" value=' + data[i][0] + ' style="background-image:' + img + ' " '+
               ' onclick="alert(this.value)"></td></tr></tbody>')
         }
      },"json")
}
